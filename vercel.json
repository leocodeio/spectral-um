{
  "version": 2,
  "buildCommand": "pnpm install && pnpm prisma generate --schema=./db/prisma/schema.prisma && pnpm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
