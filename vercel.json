{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "cd src/db && npx prisma generate && cd ../.. && pnpm run build",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
