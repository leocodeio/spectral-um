{
  "buildCommand": "cd ../.. && pnpm turbo build --filter=@spectral/api...",
  "installCommand": "cd ../.. && pnpm install --shamefully-hoist",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}
